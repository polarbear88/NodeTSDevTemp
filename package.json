{
  "name": "node_ts_dev_temp",
  "version": "1.0.0",
  "description": "NodeJs TypeScript 开发模版",
  "main": "App.ts",
  "scripts": {
    "start": "./node_modules/typescript/bin/tsc && node ./dist/App.js",
    "test": "./node_modules/typescript/bin/tsc && node ./dist/test/Index.js",
    "build": "./node_modules/typescript/bin/tsc",
    "obf": "./node_modules/javascript-obfuscator/bin/javascript-obfuscator ./bin/index.js --output ./bin/index-en.js --string-array-encoding rc4 --compact true",
    "ncc": "ncc build ./dist/App.js -o ./bin/ && cp -r ./public ./bin/public",
    "bytenode": "bytenode --compile ./bin/index-en.js && cp ./build.js ./bin/build.js",
    "pack": "npm run build && npm run ncc && npm run obf && npm run bytenode"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polarbear88/NodeTSDevTemp.git"
  },
  "author": "polarbear",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/polarbear88/NodeTSDevTemp/issues"
  },
  "homepage": "https://github.com/polarbear88/NodeTSDevTemp#readme",
  "dependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "bytenode": "^1.3.4",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "eslint": "^8.9.0",
    "express": "^4.17.3",
    "javascript-obfuscator": "^4.0.0",
    "log4js": "^6.4.1",
    "md5-node": "^1.0.1",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "string-random": "^0.1.3",
    "typeorm": "^0.2.44",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2"
  }
}